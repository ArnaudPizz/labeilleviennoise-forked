{
  "name": "@labeilleviennoise/eslint-config",
  "version": "0.15.0",
  "private": true,
  "description": "The ESLint config of l'Abeille Viennoise",
  "homepage": "https://github.com/XavierChevalier/labeilleviennoise#readme",
  "main": "lib/eslint-config.js",
  "directories": {
    "lib": "lib"
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "lint": "eslint \"**/*.{ts,tsx,js,jsx,mjs,cjs}\"",
    "lint:fix": "pnpm lint --fix",
    "lint-staged": "lint-staged"
  },
  "devDependencies": {
    "@labeilleviennoise/prettier-config": "workspace:*",
    "@remix-run/eslint-config": "^1.9.0",
    "@typescript-eslint/eslint-plugin": "^5.47.0",
    "@typescript-eslint/parser": "^5.47.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1"
  },
  "lint-staged": {
    "*.{ts,tsx,js,mjs,cjs}": "pnpm eslint --cache --fix",
    "*": "prettier -w -u"
  }
}
