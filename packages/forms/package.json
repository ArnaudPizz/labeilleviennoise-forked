{
  "name": "@labeilleviennoise/forms",
  "description": "Forms components and handlers for l'Abeille Viennoise",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "directories": {
    "lib": "dist"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm build:clean && concurrently \"pnpm:build:swc\" \"pnpm:build:tsc\" && tsc-alias",
    "build:swc": "swc src -d dist",
    "build:tsc": "tsc",
    "build:clean": "rimraf dist"
  },
  "dependencies": {
    "@labeilleviennoise/merge-classes": "workspace:*",
    "@remix-run/react": "^1.9.0",
    "@remix-run/node": "^1.9.0",
    "remix-forms": "^1.4.0",
    "zod": "^3.20.2"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.59",
    "@swc/core": "^1.3.24",
    "concurrently": "^7.6.0",
    "rimraf": "^3.0.2",
    "tsc-alias": "^1.8.2"
  }
}
