{
  "name": "@labeilleviennoise/auth",
  "description": "This package is used to authenticate user with Google",
  "version": "0.15.0",
  "private": true,
  "sideEffects": false,
  "homepage": "https://github.com/XavierChevalier/labeilleviennoise#readme",
  "main": "dist/auth.js",
  "typings": "dist/auth.d.ts",
  "directories": {
    "dist": "dist"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "pnpm prisma:generate",
    "build": "pnpm clean && pnpm prisma:generate && tsc --build",
    "lint": "eslint \"**/*.{ts,tsx,js,jsx,mjs,cjs}\"",
    "lint:fix": "pnpm lint --fix",
    "lint-staged": "lint-staged",
    "clean": "rimraf dist",
    "prisma:generate": "npx prisma generate"
  },
  "dependencies": {
    "@remix-run/node": "^1.9.0",
    "remix-auth": "^3.4.0",
    "remix-auth-oauth2": "^1.5.0",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@labeilleviennoise/eslint-config": "workspace:*",
    "@labeilleviennoise/prettier-config": "workspace:*",
    "@remix-run/react": "^1.9.0",
    "@remix-run/server-runtime": "^1.9.0",
    "eslint": "^8.30.0",
    "lint-staged": "^13.1.0",
    "prettier": "^2.8.1",
    "rimraf": "^3.0.2"
  },
  "lint-staged": {
    "*.{ts,tsx,js,mjs,cjs}": "pnpm eslint --cache --fix",
    "*": "prettier -w -u"
  }
}
