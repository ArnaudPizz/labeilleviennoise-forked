{
  "name": "@labeilleviennoise/auth",
  "description": "This package is used to authenticate user with Google",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "directories": {
    "lib": "dist"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "pnpm prisma:generate",
    "build": "pnpm build:clean && concurrently \"pnpm:build:swc\" \"pnpm:build:tsc\" && tsc-alias",
    "build:swc": "swc src -d dist",
    "build:tsc": "tsc",
    "build:clean": "rimraf dist",
    "prisma:generate": "npx prisma generate"
  },
  "dependencies": {
    "@remix-run/node": "^1.9.0",
    "remix-auth": "^3.4.0",
    "remix-auth-oauth2": "^1.5.0",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@remix-run/react": "^1.9.0",
    "@remix-run/server-runtime": "^1.9.0",
    "@swc/cli": "^0.1.59",
    "@swc/core": "^1.3.24",
    "concurrently": "^7.6.0",
    "rimraf": "^3.0.2",
    "tsc-alias": "^1.8.2"
  }
}
