{
  "name": "@labeilleviennoise/identifier",
  "description": "Utilities to create or manage identifiers",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "directories": {
    "lib": "dist"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm build:clean && concurrently \"pnpm:build:swc\" \"pnpm:build:tsc\" && tsc-alias",
    "build:swc": "swc src -d dist",
    "build:tsc": "tsc",
    "build:clean": "rimraf dist"
  },
  "devDependencies": {
    "@labeilleviennoise/eslint-config": "workspace:*",
    "@labeilleviennoise/prettier-config": "workspace:*",
    "@swc/cli": "^0.1.59",
    "@swc/core": "^1.3.24",
    "concurrently": "^7.6.0",
    "rimraf": "^3.0.2",
    "tsc-alias": "^1.8.2"
  }
}
