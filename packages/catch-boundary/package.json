{
  "name": "@labeilleviennoise/catch-boundary",
  "description": "Catch boundary components and handlers of l'Abeille Viennoise",
  "version": "0.0.0",
  "private": true,
  "sideEffects": false,
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "directories": {
    "lib": "dist"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm build:clean && concurrently \"pnpm:build:swc\" \"pnpm:build:tsc\" && tsc-alias",
    "build:swc": "swc src -d dist",
    "build:tsc": "tsc",
    "build:clean": "rimraf dist"
  },
  "peerDependencies": {
    "tailwindcss": "^3.2.4"
  },
  "dependencies": {
    "@labeilleviennoise/layouts": "workspace:*",
    "@labeilleviennoise/seo": "workspace:*",
    "@labeilleviennoise/ui": "workspace:*",
    "@remix-run/react": "^1.9.0",
    "flowbite": "^1.6.0",
    "flowbite-react": "^0.3.7",
    "imagekitio-react": "^2.0.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.59",
    "@swc/core": "^1.3.24",
    "concurrently": "^7.6.0",
    "rimraf": "^3.0.2",
    "tsc-alias": "^1.8.2"
  }
}
