{
  "name": "labeilleviennoise",
  "version": "0.0.1",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "pnpm build:css && remix build",
    "build:css": "tailwindcss -m -i ./app/assets/styles/app.css -o app/assets/styles/app.generated.css",
    "prebuild": "test \"$CI\" = true && npx pnpm install -r --store=node_modules/.pnpm-store || echo skipping pnpm install",
    "dev": "concurrently \"pnpm dev:css\" \"remix dev\"",
    "dev:css": "tailwindcss -w -i ./app/assets/styles/app.css -o app/assets/styles/app.generated.css",
    "start": "cross-env NODE_ENV=production netlify dev",
    "lint": "eslint \"**/*.{ts,tsx,js,jsx,mjs,cjs}\"",
    "lint:fix": "pnpm lint --fix",
    "test": "pnpm test:unit",
    "test:unit": "jest",
    "prepare": "husky install"
  },
  "dependencies": {
    "@netlify/functions": "^1.3.0",
    "@remix-run/netlify": "^1.7.2",
    "@remix-run/node": "^1.7.2",
    "@remix-run/react": "^1.7.2",
    "cross-env": "^7.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.1.0",
    "@next-boost/hybrid-disk-cache": "^0.3.0",
    "@remix-run/dev": "^1.7.2",
    "@remix-run/eslint-config": "^1.7.2",
    "@remix-run/serve": "^1.7.2",
    "@tailwindcss/typography": "^0.5.7",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "autoprefixer": "^10.4.12",
    "concurrently": "^7.4.0",
    "eslint": "^8.25.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "flowbite": "^1.5.3",
    "flowbite-react": "^0.2.0",
    "husky": "^8.0.1",
    "jest": "^29.2.1",
    "jest-extended": "^3.1.0",
    "lint-staged": "^13.0.3",
    "postcss": "^8.4.18",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "remix-image": "^1.3.3",
    "tailwindcss": "^3.1.8",
    "ts-jest": "^29.0.3",
    "typescript": "^4.8.4"
  },
  "lint-staged": {
    "*.{ts,tsx,js,mjs,cjs}": "pnpm eslint --cache --fix",
    "*": "prettier -w -u"
  },
  "engines": {
    "node": ">=14"
  }
}
